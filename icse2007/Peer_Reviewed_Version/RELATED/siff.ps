% leaves: ...
/edgelinedraw
{
	/doarrow makearrows def
	/docurve makecurves def

	counttomark 4 le
	{	% `simple' atomic edge
		basicedge
	}
	{	% multi-point edge
		/isfirstsegment true def
		{
			% always make a segemented egde curved
			true %makecurves
			{	curve_segment
